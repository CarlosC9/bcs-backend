[
   "no converter for model class DataToolParameter",
   {
      key: "filename",
      type: "textarea",
      templateOptions: {
         label: "Output filename",
         required: true,
         description: "Output files will be named this with .p, .t, .mcmc extensions. Use to avoid overwriting files when running repeated analyses on the same dataset."
      },
      defaultValue: ""
   },
   {
      key: "ngen",
      type: "input",
      templateOptions: {
         label: "Number of generations",
         required: true,
         description: "",
         type: "number"
      },
      defaultValue: "10000"
   },
   {
      key: "nchain",
      type: "input",
      templateOptions: {
         label: "Number of chains",
         required: true,
         description: "",
         type: "number"
      },
      defaultValue: "2"
   },
   {
      key: "nrun",
      type: "input",
      templateOptions: {
         label: "Number of runs",
         required: true,
         description: "",
         type: "number"
      },
      defaultValue: "2"
   },
   {
      key: "outgroup",
      type: "textarea",
      templateOptions: {
         label: "Outgroup",
         required: true,
         description: "The name or number identifying one taxon from the data."
